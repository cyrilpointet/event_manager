{
    "name": "event_manager",
    "description": "an event manager",
    "version": "1.0.0",
    "engines": {
        "node": "14.x"
    },
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
