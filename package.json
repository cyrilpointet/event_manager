        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-vue": "^7.7.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "prettier": "^2.2.1",
        "resolve-url-loader": "^3.1.2",
